file = {:C$\backslash$:/Users/dsbjr/Documents/Mendeley Desktop/Winter et al. - 2017 - Breakdown of magnons in a strongly spin-orbital coupled magnet.pdf:pdf},
isbn = {4146701701177},
issn = {20411723},
journal = {Nature Communications},
number = {1},
