@article{Wang2017,
abstract = {We report on terahertz spectroscopy of quantum spin dynamics in $\alpha$-RuCl 3 , a system proximate to the Kitaev honeycomb model, as a function of temperature and magnetic field. We follow the evolution of an extended magnetic continuum below the structural phase transition at T s2 ¼ 62 K. With the onset of a long-range magnetic order at T N ¼ 6.5 K, spectral weight is transferred to a well-defined magnetic excitation at ℏ$\omega$ 1 ¼ 2.48 meV, which is accompanied by a higher-energy band at ℏ$\omega$ 2 ¼ 6.48 meV. Both excitations soften in a magnetic field, signaling a quantum phase transition close to B c ¼ 7 T, where a broad continuum dominates the dynamical response. Above B c , the long-range order is suppressed, and on top of the continuum, emergent magnetic excitations evolve. These excitations follow clear selection rules and exhibit distinct field dependencies, characterizing the dynamical properties of a possibly field-induced quantum spin liquid.},
annote = {TERAHERTZ SPECTROSCOPY

Observe supression of LRMO with in-plane magnetic field. Also see magnetic excitations that follow selection rules.},
archivePrefix = {arXiv},
arxivId = {1706.06157},
author = {Wang, Zhe and Reschke, S. and H{\"{u}}vonen, D. and Do, S. H. and Choi, K. Y. and Gensch, M. and Nagel, U. and R{\~{o}}{\~{o}}m, T. and Loidl, A.},
doi = {10.1103/PhysRevLett.119.227202},
eprint = {1706.06157},
file = {:C$\backslash$:/Users/dsbjr/Documents/Mendeley Desktop/Wang et al. - 2017 - Magnetic Excitations and Continuum of a Possibly Field-Induced Quantum Spin Liquid in $\alpha$-RuCl3.pdf:pdf},
issn = {10797114},
journal = {Physical Review Letters},
number = {22},
pages = {1--6},
title = {{Magnetic Excitations and Continuum of a Possibly Field-Induced Quantum Spin Liquid in $\alpha$-RuCl3}},
volume = {119},
year = {2017}
}
