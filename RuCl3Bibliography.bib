year = {2017}
}
@article{Hou2017,
abstract = {Recent experiments reveal that the honeycomb ruthenium trichloride {\{}$\backslash$alpha{\}}-RuCl3 is a prime candidate of the Kitaev quantum spin liquid (QSL). However, there is no theoretical model which can properly describe its experimental dynamical response, due to the lack of a full understanding of its magnetic interactions. Here, we propose a general scheme to calculate the magnetic interactions in systems (e.g., {\{}$\backslash$alpha{\}}-RuCl3) with non-negligible orbital moments by constraining the directions of orbital moments. With this scheme, we put forward a minimal J1-K1-{\{}$\backslash$Gamma{\}}1-J3-K3 model for {\{}$\backslash$alpha{\}}-RuCl3 and find that: (I) The third nearest neighbor (NN) antiferromagnetic Heisenberg interaction J3 stabilizes the zigzag antiferromagnetic order; (II) The NN symmetric off-diagonal exchange {\{}$\backslash$Gamma{\}}1 plays a pivotal role in determining the preferred direction of magnetic moments and generating the spin wave gap. Exact diagonalization study on this model shows that the Kitaev QSL can be realized by suppressing the NN symmetric off-diagonal exchange {\{}$\backslash$Gamma{\}}1 and the third NN Heisenberg interaction J3. Thus, we not only propose a powerful general scheme for investigating the intriguing magnetism of Jeff=1/2 magnets, but also point out future directions for realizing the Kitaev QSL in the honeycomb ruthenium trichloride {\{}$\backslash$alpha{\}}-RuCl3.},
annote = {COMPUTATION/THEORY

Paper suggests we'll do better to realize a Kitaev QSL in RuCl3 by suppressing the "nearest neighbor symmetric off-diagonal exchange and third nearest neighbor heisenberg interaction." You can weaken these effects by strain, pressure, isovalent ion doping.},
archivePrefix = {arXiv},
arxivId = {1612.00761},
author = {Hou, Y. S. and Xiang, H. J. and Gong, X. G.},
doi = {10.1103/PhysRevB.96.054410},
eprint = {1612.00761},
file = {:C$\backslash$:/Users/dsbjr/Documents/Mendeley Desktop/Hou, Xiang, Gong - 2017 - Unveiling magnetic interactions of ruthenium trichloride via constraining direction of orbital moments Potenti.pdf:pdf},
issn = {24699969},
journal = {Physical Review B},
number = {5},
pages = {1--10},
title = {{Unveiling magnetic interactions of ruthenium trichloride via constraining direction of orbital moments: Potential routes to realize a quantum spin liquid}},
volume = {96},
year = {2017}
}
@article{Winter2017,
abstract = {The description of quantized collective excitations stands as a landmark in the quantum theory of condensed matter. A prominent example occurs in conventional magnets, which support bosonic magnons - quantized harmonic fluctuations of the ordered spins. In striking contrast is the recent discovery that strongly spin-orbital coupled magnets, such as {\$}\backslashalpha{\$}-RuCl{\$}{\_}3{\$}, may display a broad excitation continuum inconsistent with conventional magnons. Due to incomplete knowledge of the underlying interactions unraveling the nature of this continuum remains challenging. The most discussed explanation refers to a coherent continuum of fractional excitations analogous to the celebrated Kitaev spin liquid. Here we present a more general scenario. We propose that the observed continuum represents incoherent excitations originating from strong magnetic anharmoniticity that naturally occurs in such materials. This scenario fully explains the observed inelastic magnetic response of {\$}\backslashalpha{\$}-RuCl{\$}{\_}3{\$} and reveals the presence of nontrivial excitations in such materials extending well beyond the Kitaev state.},
annote = {COMPUTATION/THEORY

Can't quite tell with thiis paper, but it seems to argue that a KQSL is not necessary to explain the continuum of magnetic excitations (i.e., the continuum doesn't have to be caused by fractionalized excitations). "... due to strong coupling of one-magnon and two-magnon states" the conventional theory of magnons breaks down in the presence of the kitaev and off diagonal terms, giving rise to a continuum of excitations.},
