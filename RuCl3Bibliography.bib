@article{Hentrich2017,
abstract = {The honeycomb Kitaev-Heisenberg model is a source of a quantum spin liquid with Majorana fermions and gauge flux excitations as fractional quasiparticles. In the quest of finding a pertinent material, {\$}\backslashalpha{\$}-RuCl{\$}{\_}{\{}3{\}}{\$} recently emerged as a prime candidate. Here we unveil highly unusual low-temperature heat conductivity {\$}\backslashkappa{\$} of {\$}\backslashalpha{\$}-RuCl{\$}{\_}{\{}3{\}}{\$}: beyond a magnetic field of {\$}B{\_}c\backslashapprox{\$} 7.5 T, {\$}\backslashkappa{\$} increases by about one order of magnitude, resulting in a large magnetic field dependent peak at about 7 K, both for in-plane as well as out-of-plane transport. This clarifies the unusual magnetic field dependence unambiguously to be the result of severe scattering of phonons off putative Kitaev-Heisenberg excitations in combination with a drastic field-induced change of the magnetic excitation spectrum. In particular, an unexpectedly large energy gap arises, which increases approximately linearly with the magnetic field and reaches a remarkably large {\$}\backslashhbar\backslashomega{\_}0/k{\_}B\backslashapprox {\$} 50 K at 18 T.},
annote = {THERMAL CONDUCTIVITY

Observe a large increase in thermal conductivity as temperature decreases with a large, in-plane magnetic field. Behavior changes profoundly near 7.5T.

Similar behavior in thermal conductivity is seen in-plane and out-of-plane, ruling out a strictly magnetic contribution to thermal conductivity (as RuCl3 is a 2D system). Paper asserts that the low-temp enhancement is due to field dependent scattering of phonons, which carry the heat.},
archivePrefix = {arXiv},
arxivId = {1703.08623},
author = {Hentrich, Richard and Wolter, Anja U. B. and Zotos, Xenophon and Brenig, Wolfram and Nowak, Domenic and Isaeva, Anna and Doert, Thomas and Banerjee, Arnab and Lampen-Kelley, Paula and Mandrus, David G. and Nagler, Stephen E. and Sears, Jennifer and Kim, Young-June and B{\"{u}}chner, Bernd and Hess, Christian},
