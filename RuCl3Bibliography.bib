abstract = {Quantum fluctuations in a 2D honeycomb Kitaev model completely suppress long-range order to produce a quantum spin liquid (QSL) ground state. While {\$}\backslashalpha{\$}-RuCl{\$}{\_}{\{}3{\}}{\$} is positioned on the verge of the Kitaev QSL phase, fragile long-range magnetic order arises from non-Kitaev terms in the Hamiltonian. In a study of Ru{\$}{\_}{\{}1-x{\}}{\$}Ir{\$}{\_}{\{}x{\}}{\$}Cl{\$}{\_}{\{}3{\}}{\$} single crystals, we find that spin vacancies in the form of nonmagnetic Ir{\$}{\^{}}{\{}3+{\}}{\$} successfully destabilize magnetic order in {\$}\backslashalpha{\$}-RuCl{\$}{\_}{\{}3{\}}{\$}. Neutron diffraction and bulk characterization demonstrate a suppression of the primary zig-zag magnetic phase transition in Ru{\$}{\_}{\{}1-x{\}}{\$}Ir{\$}{\_}{\{}x{\}}{\$}Cl{\$}{\_}{\{}3{\}}{\$} single crystals to below 2 K by {\$}x = 0.09{\$}, potentially paving the way towards the sought-after pure Kitaev limit. Stacking disordered regions exhibit less sensitivity to dilution, and sustain weak zig-zag correlations to larger {\$}x{\$}, near the percolation threshold for the honeycomb lattice. The phase diagram as a function of {\$}x{\$} and T is determined.},
annote = {MAGNETIZATION, NEUTRON DIFFRACTION, SPECIFIC HEAT

This paper substitutes Ir for Ru and shows that it destabilizes the LRMO for substitution factor {\textgreater} 0.3. Basically, you can make a RuCl3 into a Kitaev QSL by substituting in Ir, even without an in-plane field. Though in this case, it is a dilute QSL.},
archivePrefix = {arXiv},
arxivId = {1612.07202},
author = {Lampen-Kelley, P. and Banerjee, A. and Aczel, A. A. and Cao, H. B. and Stone, M. B. and Bridges, C. A. and Yan, J. Q. and Nagler, S. E. and Mandrus, D.},
doi = {10.1103/PhysRevLett.119.237203},
eprint = {1612.07202},
