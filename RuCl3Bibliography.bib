file = {:C$\backslash$:/Users/dsbjr/Documents/Mendeley Desktop/Winter et al. - 2017 - Models and Materials for Generalized Kitaev Magnetism.pdf:pdf},
title = {{Models and Materials for Generalized Kitaev Magnetism}},
url = {https://arxiv.org/pdf/1706.06113.pdf},
year = {2017}
