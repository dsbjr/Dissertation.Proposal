arxivId = {1705.06139},
author = {Baek, S. H. and Do, S. H. and Choi, K. Y. and Kwon, Y. S. and Wolter, A. U.B. and Nishimoto, S. and {Van Den Brink}, Jeroen and B{\"{o}}chner, B.},
doi = {10.1103/PhysRevLett.119.037201},
eprint = {1705.06139},
